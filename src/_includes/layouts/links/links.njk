<!doctype html>
<html class="no-js" lang="en">
  <head>
    {% include "layouts/head.njk" %}
    <link rel="stylesheet" href="/src/css/main.min.css">
    <link rel="stylesheet" href="/src/css/links.min.css">
  </head>
  <body>
    <div id="linksCover">
      <img src="/src/img/links/banner-{{ coverImg.img }}" alt="{{ coverImg.imgAlt }}">
    </div>
    <div id="linksPFPs">
      {% for pfp in linksPFPs %}
        <img src="/src/img/links/pfp-{{ pfp.img }}" alt="{{ pfp.imgAlt }}">
      {% endfor %}
    </div>
    <main>
      {{ content | safe }}
    </main>
    <footer>
      {% include "layouts/footer-copyright.njk" %}
    </footer>
    {% include "layouts/scripts.njk" %}
  </body>
</html>